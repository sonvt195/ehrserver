        <term_definitions code="at0000">
          <items id="text">Problema/diagnóstico</items>
          <items id="description">Detalles de una condición o problema de salud que puede afectar física, mental o socialmente al paciente y es identificado por un profesional clínico.</items>
        </term_definitions>
        <term_definitions code="at0001">
          <items id="text">structure</items>
          <items id="description">@ internal @</items>
        </term_definitions>
        <term_definitions code="at0002">
          <items id="text">Nombre del problema/diagnóstico</items>
          <items id="description">Identificación por nombre y/o código del problema/diagnóstico</items>
        </term_definitions>
        <term_definitions code="at0003">
          <items id="text">Momento de reconocimiento del problema</items>
          <items id="description">Fecha y hora, estimado o real, cuándo el problema/diagnóstico es detectado por un profesional de la salud</items>
        </term_definitions>
        <term_definitions code="at0005">
          <items id="text">Severidad</items>
          <items id="description">Valoración de la severidad del problema/diagnóstico</items>
        </term_definitions>
        <term_definitions code="at0009">
          <items id="text">Descripción clínica</items>
          <items id="description">Descripción narrativa del problema/diagnóstico</items>
        </term_definitions>
        <term_definitions code="at0012">
          <items id="text">Ubicación anatómica</items>
          <items id="description">Lugar del cuerpo donde se detecta el problema/diagnóstico</items>
        </term_definitions>
        <term_definitions code="at0027">
          <items id="text">Elemento relacionado</items>
          <items id="description">Identificación de otros problemas/diagnósticos que están relacionados al problema/diagnóstico actual</items>
        </term_definitions>
        <term_definitions code="at0028">
          <items id="text">Elemento</items>
          <items id="description">Identificación del problema/diagnóstico relacionado</items>
        </term_definitions>
        <term_definitions code="at0029">
          <items id="text">Relación</items>
          <items id="description">Tipo de relación que este problema/diagnóstico tiene con el elemento relacionado</items>
        </term_definitions>
        <term_definitions code="at0030">
          <items id="text">Momento de resolución</items>
          <items id="description">Día y hora, estimado o real, en que el problema/diagnóstico fue resuelto o entró en remisión</items>
        </term_definitions>
        <term_definitions code="at0032">
          <items id="text">Tree</items>
          <items id="description">@ internal @</items>
        </term_definitions>
        <term_definitions code="at0039">
          <items id="text">Ubicación anatómica estructurada</items>
          <items id="description">Este espacio permite registrar información estructurada sobre la ubicación anatómica</items>
        </term_definitions>
        <term_definitions code="at0040">
          <items id="text">Causa</items>
          <items id="description">Causa directa o indirecta del problema/diagnóstico</items>
        </term_definitions>
        <term_definitions code="at0041">
          <items id="text">Secuencia</items>
          <items id="description">Este valor identifica la secuencia de eventos entre elementos relacionados, y puede ser utilizado para describir secuelas o efectos tardíos</items>
        </term_definitions>
        <term_definitions code="at0043">
          <items id="text">Detalles específicos</items>
          <items id="description">Detalles adicionales para el problema/diagnóstico</items>
        </term_definitions>
        <term_definitions code="at0044">
          <items id="text">Descripción</items>
          <items id="description">Descripción narrativa del elemento relacionado</items>
        </term_definitions>
        <term_definitions code="at0046">
          <items id="text">Estatus</items>
          <items id="description">Detalles estructurados de la ubicación, dominio, episodio o flujo de trabajo de aspectos específicos del proceso diagnóstico</items>
        </term_definitions>
        <term_definitions code="at0047">
          <items id="text">Leve</items>
          <items id="description">El problema tiene severidad leve</items>
        </term_definitions>
        <term_definitions code="at0048">
          <items id="text">Moderada</items>
          <items id="description">El problema tiene severidad moderada</items>
        </term_definitions>
        <term_definitions code="at0049">
          <items id="text">Severo</items>
          <items id="description">El problema tiene severidad severo</items>
        </term_definitions>
        <term_definitions code="at0069">
          <items id="text">Comentario</items>
          <items id="description">Comentario narrativo adicional sobre el problema/diagnóstico no capturado en otros campos</items>
        </term_definitions>
        <term_definitions code="at0070">
          <items id="text">Última actualización</items>
          <items id="description">Fecha en la que el problema/diagnóstico fue actualizado</items>
        </term_definitions>
        <term_definitions code="at0071">
          <items id="text">Extensión</items>
          <items id="description">Información adicional requerida para capturar el contenido local o alinear con otros modelos o formalismos para el problema/diagnóstico</items>
